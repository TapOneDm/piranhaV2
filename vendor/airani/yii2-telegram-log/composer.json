{
    "name": "airani/yii2-telegram-log",
    "description": "Yii 2.0 telegram log target, send logs to the telegram chats or channels",
    "keywords": ["yii2", "telegram", "bot", "log"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/airani/yii2-telegram-log/issues",
        "source": "https://github.com/airani/yii2-telegram-log",
        "irc": "irc://irc.freenode.net/yii"
    },
    "authors": [
        {
            "name": "Ali Irani",
            "email": "ali@irani.im"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-httpclient": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "airani\\log\\": ""
        }
    }
